---
layout: null
---
{% assign pages = site.pages | sort: 'title' %}
{
  {% for page in pages %}
	{% assign pageurl = page.url | slugify %}
	{% if page.title != null and page.title != empty and pageurl != null and pageurl != empty %}
    "{{ pageurl }}": {
      "title": "{{ page.title | xml_escape }}",
      "url": "{{ page.url | xml_escape }}",
	  "content": "{{ page.content | strip_html | escape | strip | strip_newlines | replace : '	', '' }}"
    }
   {% unless forloop.last %},{% endunless %}
 	{% endif %}
  {% endfor %}
}